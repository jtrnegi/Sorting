#include<stdio.h>
#include<conio.h>
void main()
{
int arr[6]={12,16,23,7,8,44};
int i,j,n=6;

for(i=0;i<n-1;i++)
{
  for{j=0;j<n-i-1;j++)
  {
  if(arr[j] < arr[j+1])
  {
  int temp = arr[j];
  arr[j]=arr[j+1];
  arr[j+1]=temp;
  }}
  }
printf("the sorted version of list");  
for(i=0;i<n;i++)
{
printf("%d \n",arr[i]);
}

getch();
}
